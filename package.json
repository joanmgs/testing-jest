    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "react-test-renderer": "^17.0.2"
  }
}
